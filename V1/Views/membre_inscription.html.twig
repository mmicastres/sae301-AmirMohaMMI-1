{% extends "index.html.twig" %}

{% block section %}
<form method="post" action="index.php" class="well">
<fieldset>
  <legend>veuillez vous identifiez</legend>
    <div class="form-group row">
      <label for="nom" class="col-sm-2 col-form-label col-form-label-sm">nom</label>
      <div class="col-sm-6">
        <input  type="text" id="nom" class="form-control form-control-sm" name="nom" required placeholder="" />
      </div>
    </div>
    <div class="form-group row">
      <label for="prenom" class="col-sm-2 col-form-label col-form-label-sm">prenom</label>
      <div class="col-sm-6">
        <input  type="text" id="prenom" class="form-control form-control-sm" name="prenom" required placeholder="" />
      </div>
    </div>
    <div class="form-group row">
      <label for="idiut" class="col-sm-2 col-form-label col-form-label-sm">Identifiant IUT</label>
      <div class="col-sm-6">
        <input  type="password" id="idiut" class="form-control form-control-sm" name="idiut" required placeholder="" />
      </div>
    </div>
    
    <div class="form-group row">
      <label for="email" class="col-sm-2 col-form-label col-form-label-sm">Email</label>
      <div class="col-sm-6">
        <input  type="text" id="email" class="form-control form-control-sm" name="email" required placeholder="" />
      </div>
    </div>
     <input  type="hidden" id="idutilisateur" name="idutilisateur" value="{{ idutilisateur }}"/> 
    
  <input type="submit" name="inscription" value="inscription"/>
</fieldset>
</form>
{% endblock %}